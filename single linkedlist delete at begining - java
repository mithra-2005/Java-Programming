class Linkedlist {
    Node head;

    static class Node {
        int data;
        Node next;

        Node(int data) {
            this.data = data;
            this.next = null;
        }
    }

    static Linkedlist insert(Linkedlist list, int data) {
        Node newnode = new Node(data);

        if (list.head == null) {
            list.head = newnode;
        } else {
            Node temp = list.head;
            while (temp.next != null) {
                temp = temp.next;
            }
            temp.next = newnode;
        }
        return list;
    }

    static void deleteBeginning(Linkedlist list) {
        if (list.head == null) {
            return;
        }
        list.head = list.head.next;
    }

    static void display(Linkedlist list) {
        Node temp = list.head;
        while (temp != null) {
            System.out.print(temp.data + " --> ");
            temp = temp.next;
        }
        System.out.print("NULL");
    }

    public static void main(String[] args) {
        Linkedlist list = new Linkedlist();

        insert(list, 35);
        insert(list, 40);
        insert(list, 45);
        insert(list, 50);
        insert(list, 55);

        deleteBeginning(list);  
        display(list);           
    }
}

