public class Main
{
	public static void main(String[] args) {
	       String s = "Mithra";
	       char arr[] = s.toCharArray();
	       int left = 0 , right = arr.length - 1;
	       char temp;
	       while(left <= right)
	       {
	           temp = arr[left];
	           arr[left] = arr[right];
	           arr[right] = temp;
	           left++;
	           right--;
	       }
	       System.out.print(new String(arr));
	}
}
 
