import java.util.*;
public class Main {
    public static void main(String[] args) {
        int arr[] = {12 , 3 , 4 , 98 , 6, 43};
        int k = 98;
        int i;

        int len = arr.length;
        for( i = 0 ; i < len ; i++) 
        {
            if(arr[i] == k)
            {
                System.out.println("Elemnt Found at " + i);
                break;
            }
            
            
        }
    }
}
