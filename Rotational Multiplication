import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int digit = 1;
        int rem;
        int product;
        while ( n != 0){
            rem = n % 10;
            product = digit * rem;
            System.out.print(product + " ");
            n = n / 10;
            digit++;
        }
        
    }
}
