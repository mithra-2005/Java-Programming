class node {
    int data;
    node next;
    node prev;
    node(int data) {
        this.data = data;
        this.next = null;
        this.prev = null;
    }
}
public class Main {
static node head = null;
static node tail = null;
static void insert(int data) {
    node newnode = new node(data);
    if(tail == null) {
        head = newnode;
        tail = newnode;
    } else {
        tail.next = newnode;
        newnode.prev = tail;
        tail = newnode;
    }
}
static void display()
{
    node temp = head;
    while(temp != null)
    {
        System.out.print(temp.data + " - >" );
        temp = temp.next;
    }
    System.out.print("NULL");
}
	public static void main(String[] args) {
	    insert(100);
	    insert(200);
	    insert(300);
	    insert(400);
	    display();
	
	}
}
